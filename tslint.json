{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "no-bitwise": false,
      "forin": false,
      "prefer-for-of": true,
      "prefer-object-spread": true,
      "restrict-plus-operands": true,
      "cyclomatic-complexity": true,
      "completed-docs": true,
      "trailing-comma": false
    },
    "rulesDirectory": []
}
